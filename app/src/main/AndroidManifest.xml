        <activity android:name=".Activity.BookingActivity"
            android:theme="@style/Theme.PKCP.NoActionBar">
        </activity>
        <activity android:name=".Activity.PropertyInfoActivity"
            android:theme="@style/Theme.PKCP.NoActionBar">
        </activity>
        <activity android:name=".Activity.ChangePasswordActivity"
            android:theme="@style/Theme.PKCP.NoActionBar">
        </activity>
        <activity android:name=".WebRTC.RTCActivity"
            android:theme="@style/Theme.PKCP.NoActionBar">
        </activity>
    </application>

</manifest>